<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" type="text/css" href="styleSearch.css">
  </head>
<body>
  <h1><%= title %></h1>
  <% if (message) { %>
        <p> <%= message %> </p>
  <% } %>
  <ul>
    <% if (locals.users) {%>
      <% users.forEach(user => { %>
        <li>
          ID: <%= user.id %>, Name: <%= user.firstName %> <%= user.lastName %>
        </li>
      <% }); %>
    <% } %>
  </ul>
  <a href="/">Back to home</a>
</body>
</html>
